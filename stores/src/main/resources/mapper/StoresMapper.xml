<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="xyz.gaoliqing.stores.mapper.StoreMapper">

    <select id="getCounts" resultType="String">

        SELECT s_number FROM store
        where s_name = #{name}

    </select>

    <select id="getCapsule" resultType="xyz.gaoliqing.stores.pojo.Capsule">

        SELECT * FROM capsule

    </select>

</mapper>